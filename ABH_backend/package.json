    "@nestjs/core": "^9.4.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.0.3",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^10.3.10",
    "@redis/client": "^1.6.0",
    "aws-sdk": "^2.1635.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.2",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "ioredis": "^5.3.2",
    "mailtrap": "^3.3.0",
    "mime-types": "^2.1.35",
    "mongoose": "^8.5.2",
    "morgan": "^1.10.0",
    "nestjs-paginate": "^8.1.3",
    "nestjs-redis": "^1.3.3",
    "node-mailjet": "^6.0.4",
    "nodemailer": "^6.9.14",
    "pg": "^8.11.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
